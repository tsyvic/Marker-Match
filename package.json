{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:ci": "npm run generate && npx dotenv -e .env.local -- prisma migrate deploy && npm run db:seed && next build",
    "start": "next start",
    "lint": "next lint",
    "db:list-migrations": "npx dotenv -e .env.local -- prisma migrate status",
    "db:migrate": "npx dotenv -e .env.local -- prisma migrate dev",
    "db:new-migration": "npx dotenv -e .env.local -- prisma migrate dev --name",
    "db:reset": "npx dotenv -e .env.local -- prisma migrate reset",
    "generate": "npx dotenv -e .env.local -- pnpm run generate:prisma",
    "db:seed": "npx dotenv -e .env.local -- prisma db seed",
    "generate:prisma": "prisma generate"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.16.0",
    "@emotion/cache": "^11.10.8",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@faker-js/faker": "^8.0.1",
    "@fontsource/roboto": "^4.5.8",
    "@mui/icons-material": "^5.11.11",
    "@mui/material": "^5.11.14",
    "@mui/x-data-grid": "^6.6.0",
    "@mui/x-data-grid-pro": "^6.6.0",
    "@mui/x-date-pickers": "^6.3.0",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "4.14.1",
    "@trpc/client": "^10.28.0",
    "@trpc/next": "^10.28.0",
    "@trpc/server": "^10.28.0",
    "@types/express": "^4.17.17",
    "@types/multer": "^1.4.7",
    "@types/node": "18.15.5",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "aws-sdk": "^2.1390.0",
    "axios": "^1.4.0",
    "bcryptjs": "^2.4.3",
    "csv-writer": "^1.6.0",
    "export-to-csv": "^0.2.1",
    "formik": "^2.2.9",
    "formik-mui": "5.0.0-alpha.0",
    "google-auth-library": "^8.8.0",
    "lodash": "^4.17.21",
    "luxon": "^3.3.0",
    "next": "13.2.4",
    "next-auth": "^4.22.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-query": "^3.39.3",
    "superjson": "^1.12.3",
    "typescript": "5.0.2",
    "yup": "^1.1.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/lodash": "^4.14.194",
    "@types/luxon": "^3.3.0",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^8.39.0",
    "eslint-config-next": "^13.3.2",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prisma": "^4.14.1",
    "ts-node": "^10.9.1"
  }
}
